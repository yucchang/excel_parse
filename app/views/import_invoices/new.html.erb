<h1>ImportInvoices#new</h1>

<%= form_for @import_invoice do |f| %>
  <% if @import_invoice.errors.any?  %>
  <ul>
    <%= @import_invoice.errors.full_messages.each do |msg|  %>
    <li><%= msg %></li>
    <% end %>
  </ul>
  <% end %>
  
  <%= f.file_field :file %>
  <%= f.submit 'Upload' %>
<% end  %>
